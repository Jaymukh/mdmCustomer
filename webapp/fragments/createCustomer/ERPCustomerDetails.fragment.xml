<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:form="sap.ui.layout.form" xmlns:l="sap.ui.layout">
	<form:SimpleForm editable="true" layout="ResponsiveGridLayout" labelSpanXL="4" labelSpanL="4" labelSpanM="4" labelSpanS="12"
		adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="3" columnsL="2" columnsM="2"
		singleContainerFullSize="false">
		<form:content>
			<core:Title text="Administrative Data"/>
			<Label text="Customer"/>
			<Input editable="false" value="{Customer>/createCRCustomerData/formData/parentDTO/customData/cust_kna1/kunnr}" maxLength="10"/>
			<core:Title/>
			<Label text="Account Group"/>
			<ComboBox required="true" showSecondaryValues="true" items="{userManagementModel>/accountGroups}"
				selectedKey="{Customer>/createCRCustomerData/formData/parentDTO/customData/cust_kna1/ktokd}">
				<items>
					<core:ListItem text="{userManagementModel>text}" additionalText="{userManagementModel>key}" key="{userManagementModel>key}"/>
				</items>
			</ComboBox>
			<core:Title text="General Data"/>
			<Label text="Title"/>
			<ComboBox items="{Dropdowns>/TSAD3}" selectedKey="{Customer>/createCRCustomerData/formData/parentDTO/customData/cust_kna1/anred}">
				<items>
					<core:Item text="{Dropdowns>title_MEDI}" key="{Dropdowns>title}"/>
				</items>
			</ComboBox>
			<Label text="Name1"/>
			<Input required="true" value="{Customer>/createCRCustomerData/formData/parentDTO/customData/cust_kna1/name1}" maxLength="35"
				liveChange="handleName1"/>
			<Label text="Name2"/>
			<Input value="{Customer>/createCRCustomerData/formData/parentDTO/customData/cust_kna1/name2}" maxLength="35"/>
			<Label text="Name3"/>
			<Input value="{Customer>/createCRCustomerData/formData/parentDTO/customData/cust_kna1/name3}" maxLength="35"/>
			<core:Title/>
			<Label text="Legal Name (Name4)"/>
			<Input value="{Customer>/createCRCustomerData/formData/parentDTO/customData/cust_kna1/name4}" maxLength="35"/>
			<Label text="Natural person"/>
			<CheckBox select="onSelectCheckBox">
				<customData>
					<core:CustomData key="/cust_kna1/stkzn" value="X"></core:CustomData>
				</customData>
			</CheckBox>
			<Label text="Search Terms"/>
			<Input required="true" liveChange="handleSearchTerms" value="{Customer>/createCRCustomerData/formData/parentDTO/customData/cust_kna1/sortl}"
				maxLength="10"/>
			<core:Title text="Communication Data"/>
			<Label text="Telephone"/>
			<ComboBox items="{Dropdowns>/T005K}">
				<layoutData>
					<l:GridData span="XL2 L2 M3 S4"/>
				</layoutData>
				<core:Item key="{Dropdowns>telefto}" text="{Dropdowns>telefto} - {Dropdowns>land1}"/>
			</ComboBox>
			<Input type="Number" value="" placeholder="Phone"></Input>
			<Input type="Number" value="" placeholder="Ext"/>
			<Label text="Mobile"/>
			<ComboBox items="{Dropdowns>/T005K}">
				<layoutData>
					<l:GridData span="XL2 L2 M3 S4"/>
				</layoutData>
				<core:Item key="{Dropdowns>telefto}" text="{Dropdowns>telefto} - {Dropdowns>land1}"/>
			</ComboBox>
			<Input type="Number" value="" placeholder="Phone"></Input>
			<Label text="E-mail Address"/>
			<Input type="Email" value=""/>
			<Label text="Fax"/>
			<ComboBox items="{Dropdowns>/T005K}">
				<layoutData>
					<l:GridData span="XL2 L2 M3 S4"/>
				</layoutData>
				<core:Item key="{Dropdowns>telefto}" text="{Dropdowns>telefto} - {Dropdowns>land1}"/>
			</ComboBox>
			<Input value="" placeholder="Phone"></Input>
			<Input value="" placeholder="Ext">
				<layoutData>
					<l:GridData span="XL2 L2 M3 S4"/>
				</layoutData>
			</Input>
			<Label text="Web site"/>
			<Input editable="false" value=""/>
			<core:Title/>
			<Label text="PO Box Lobby"/>
			<Input value="{Customer>/createCRCustomerData/gen_adrc/po_box_lobby}" maxLength="40"/>
			<Label text="Type of Delivery Service"/>
			<Input value="{Customer>/createCRCustomerData/gen_adrc/deli_serv_type}" showValueHelp="true" valueHelpRequest=".onValueHelpRequested"
				valueHelpOnly="true">
				<customData>
					<core:CustomData key="title" value="Type of Delivery Service"></core:CustomData>
					<core:CustomData key="table" value="ADDRC_DELI_SERV"></core:CustomData>
					<core:CustomData key="inputKey" value="deli_SERV_TYPE"></core:CustomData>
					<core:CustomData key="inputText" value="deli_SERV_DSCR"></core:CustomData>
					<core:CustomData key="deli_SERV_TYPE" value="Delivery Type"></core:CustomData>
					<core:CustomData key="deli_SERV_DSCR" value="Description"></core:CustomData>
				</customData>
			</Input>
			<Label text="Number of Delivery Service"/>
			<Input value="{Customer>/createCRCustomerData/gen_adrc/deli_serv_number}" maxLength="4"/>
			<Label text="PO Box"/>
			<Input value="{Customer>/createCRCustomerData/formData/parentDTO/customData/cust_kna1/pfach}" maxLength="10"/>
			<Label text="Undeliverable"/>
			<Input value="{Customer>/createCRCustomerData/gen_adrc/dont_use_p}" showValueHelp="true" valueHelpRequest=".onValueHelpRequested"
				valueHelpOnly="true">
				<customData>
					<core:CustomData key="title" value="Undeliverable"></core:CustomData>
					<core:CustomData key="table" value=""></core:CustomData>
					<core:CustomData key="inputKey" value="deli_SERV_TYPE"></core:CustomData>
					<core:CustomData key="inputText" value="deli_SERV_DSCR"></core:CustomData>
					<core:CustomData key="deli_SERV_TYPE" value="Delivery Type"></core:CustomData>
					<core:CustomData key="deli_SERV_DSCR" value="Description"></core:CustomData>
				</customData>
			</Input>
			<core:Title text="Address"/>
			<Label text="Street / House No."/>
			<Input required="ture" width="100%" value="{Customer>/createCRCustomerData/gen_adrc/street}" maxLength="60"/>
			<Input required="true" width="100%" value="{Customer>/createCRCustomerData/gen_adrc/house_num1}" maxLength="10">
				<layoutData>
					<l:GridData span="XL2 L3 M3 S4"/>
				</layoutData>
			</Input>
			<Label text="District"/>
			<Input required="true" value="{Customer>/createCRCustomerData/gen_adrc/city2}" maxLength="40"/>
			<Label text="Street 2"/>
			<Input value="{Customer>/createCRCustomerData/gen_adrc/str_suppl1}" maxLength="40"/>
			<Label text="Street 3"/>
			<Input value="{Customer>/createCRCustomerData/gen_adrc/str_suppl2}" maxLength="40"/>
			<Label text="Street 4"/>
			<Input value="{Customer>/createCRCustomerData/gen_adrc/str_suppl3}" maxLength="40"/>
			<Label text="Street 5"/>
			<Input value="{Customer>/createCRCustomerData/gen_adrc/location}" maxLength="40"/>
			<Label text="Postal Code / City"/>
			<Input required="true" value="{Customer>/createCRCustomerData/formData/parentDTO/customData/cust_kna1/pstlz}" maxLength="10">
				<layoutData>
					<l:GridData span="XL2 L3 M3 S4"/>
				</layoutData>
			</Input>
			<Input required="true" value="{Customer>/createCRCustomerData/formData/parentDTO/customData/cust_kna1/ort01}" maxLength="35"/>
			<Label text="Country"/>
			<ComboBox required="true" items="{Dropdowns>/T005}"
				selectedKey="{Customer>/createCRCustomerData/formData/parentDTO/customData/cust_kna1/land1}">
				<core:Item key="{Dropdowns>land1}" text="{Dropdowns>landx}"/>
			</ComboBox>
			<core:Title/>
			<Label text="Region"/>
			<Input required="true" showValueHelp="true" valueHelpRequest=".onValueHelpRequested" valueHelpOnly="true"
				value="{Customer>/createCRCustomerData/formData/parentDTO/customData/cust_kna1/regio}">
				<customData>
					<core:CustomData key="title" value="Region"></core:CustomData>
					<core:CustomData key="table" value="T005S"></core:CustomData>
					<core:CustomData key="inputKey" value="bland"></core:CustomData>
					<core:CustomData key="inputText" value="bland"></core:CustomData>
					<core:CustomData key="bland" value="Code"></core:CustomData>
					<core:CustomData key="bezei" value="Region"></core:CustomData>
				</customData>
			</Input>
			<Label text="Language"/>
			<ComboBox required="true" selectedKey="{Customer>/createCRCustomerData/formData/parentDTO/customData/cust_kna1/spras}"
				items="{Dropdowns>/T002}">
				<core:Item key="{Dropdowns>spras}" text="{Dropdowns>sptxt}"/>
			</ComboBox>
			<Label text="Transportation Zone"/>
			<Input value="{Customer>/createCRCustomerData/formData/parentDTO/customData/cust_kna1/lzone}" showValueHelp="true"
				valueHelpRequest=".onValueHelpRequested" valueHelpOnly="true">
				<customData>
					<core:CustomData key="title" value="Transportation Zone"></core:CustomData>
					<core:CustomData key="table" value="TZONE"></core:CustomData>
					<core:CustomData key="inputKey" value="zone1"></core:CustomData>
					<core:CustomData key="inputText" value="zone1"></core:CustomData>
					<core:CustomData key="land1" value="Country Key"></core:CustomData>
					<core:CustomData key="zone1" value="Zone Code"></core:CustomData>
					<core:CustomData key="vtext" value="Transportation Zone"></core:CustomData>
				</customData>
			</Input>
			<Label text="PO Box Postal Code/ PO Box"/>
			<Input liveChange="handleERPPOBOXPostalCode" value="{Customer>/createCRCustomerData/formData/parentDTO/customData/cust_kna1/po_box}"
				maxLength="10"/>
			<Label text="PO Box Without No."/>
			<CheckBox select="onSelectCheckBox">
				<customData>
					<core:CustomData key="/gen_adrc/gen_adrc_1/po_box_num" value="X"></core:CustomData>
				</customData>
			</CheckBox>
			<Label text="Company Postal Code"/>
			<Input value="{Customer>/createCRCustomerData/gen_adrc/post_code3}" maxLength="10"/>
			<Label text="Supplement"/>
			<Input value="{Customer>/createCRCustomerData/gen_adrc/house_num2}" maxLength="10"/>
		</form:content>
	</form:SimpleForm>
</core:FragmentDefinition>