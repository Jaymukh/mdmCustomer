<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:form="sap.ui.layout.form" xmlns:l="sap.ui.layout">
	<form:SimpleForm editable="true" layout="ResponsiveGridLayout" labelSpanXL="4" labelSpanL="4" labelSpanM="4" labelSpanS="12"
		adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="3" columnsL="2" columnsM="2"
		singleContainerFullSize="false">
		<form:content>
			<core:Title text="Administrative Data"/>
			<Label text="Customer"/>
			<Input enabled="false" value="{Customer>/createCRVendorData/formData/parentDTO/customData/vnd_lfa1/lifnr}" maxLength="10"/>
			<core:Title/>
			<Label text="Account Group" required="true"/>
			<ComboBox showSecondaryValues="true" id="accountGrpID" items="{userManagementModel>/accountGroups}"
				selectedKey="{Customer>/createCRVendorData/formData/parentDTO/customData/vnd_lfa1/KTOKK}">
				<items>
					<core:ListItem text="{userManagementModel>text}" additionalText="{userManagementModel>key}" key="{userManagementModel>key}"/>
				</items>
			</ComboBox>
			<core:Title text="General Data"/>
			<Label text="Title"/>
			<ComboBox id="generalDataTitleId" items="{Dropdowns>/TSAD3}"
				selectedKey="{Customer>/createCRVendorData/formData/parentDTO/customData/vnd_lfa1/ANRED}">
				<items>
					<core:Item text="{Dropdowns>title_MEDI}" key="{Dropdowns>title}"/>
				</items>
			</ComboBox>
			<Label text="Name1"/>
			<Input id="idERPVendorName1" required="true" value="{Customer>/createCRVendorData/formData/parentDTO/customData/vnd_lfa1/NAME1}"
				maxLength="35" liveChange="handleName1"/>
			<Label text="Name2"/>
			<Input value="{Customer>/createCRVendorData/formData/parentDTO/customData/vnd_lfa1/Name2}" maxLength="35"/>
			<Label text="Name3"/>
			<Input value="{Customer>/createCRVendorData/formData/parentDTO/customData/vnd_lfa1/Name3}" maxLength="35"/>
			<core:Title/>
			<Label text="Legal Name (Name4)"/>
			<Input value="{Customer>/createCRVendorData/formData/parentDTO/customData/vnd_lfa1/Name4}" maxLength="35"/>
			<Label text="Natural person"/>
			<CheckBox select="onSelectCheckBox">
				<customData>
					<core:CustomData key="/vnd_lfa1/STKZN" value="X"></core:CustomData>
				</customData>
			</CheckBox>
			<Label text="Search Terms"/>
			<Input id="idERPVendorSearchTerms" required="true" liveChange="handleSearchTerms"
				value="{Customer>/createCRVendorData/formData/parentDTO/customData/vnd_lfa1/SORTL}" maxLength="10"/>
			<core:Title text="Communication Data"/>
			<Label text="Telephone"/>
			<ComboBox items="{Dropdowns>/T005K}">
				<layoutData>
					<l:GridData span="XL2 L2 M3 S4"/>
				</layoutData>
				<core:Item key="{Dropdowns>telefto}" text="{Dropdowns>telefto} - {Dropdowns>land1}"/>
			</ComboBox>
			<Input type="Number" value="" placeholder="Phone"></Input>
			<Input type="Number" value="" placeholder="Ext"/>
			<Label text="Mobile"/>
			<ComboBox items="{Dropdowns>/T005K}">
				<layoutData>
					<l:GridData span="XL2 L2 M3 S4"/>
				</layoutData>
				<core:Item key="{Dropdowns>telefto}" text="{Dropdowns>telefto} - {Dropdowns>land1}"/>
			</ComboBox>
			<Input type="Number" value="" placeholder="Phone"></Input>
			<Label text="E-mail Address"/>
			<Input type="Email" value=""/>
			<Label text="Fax"/>
			<ComboBox items="{Dropdowns>/T005K}">
				<layoutData>
					<l:GridData span="XL2 L2 M3 S4"/>
				</layoutData>
				<core:Item key="{Dropdowns>telefto}" text="{Dropdowns>telefto} - {Dropdowns>land1}"/>
			</ComboBox>
			<Input value="" placeholder="Phone"></Input>
			<Input value="" placeholder="Ext">
				<layoutData>
					<l:GridData span="XL2 L2 M3 S4"/>
				</layoutData>
			</Input>
			<Label text="Web site"/>
			<Input value=""/>
			<core:Title/>
			<Label text="PO Box Lobby"/>
			<Input value="{Customer>/createCRVendorData/formData/parentDTO/customData/gen_adrc/gen_adrc_1/po_box_lobby}"/>
			<Label text="Type of Delivery Service"/>
			<Input value="{Customer>/createCRVendorData/formData/parentDTO/customData/gen_adrc/gen_adrc_1/deli_serv_type}" showValueHelp="true"
				valueHelpRequest=".onValueHelpRequested" valueHelpOnly="true">
				<customData>
					<core:CustomData key="title" value="Type of Delivery Service"></core:CustomData>
					<core:CustomData key="table" value="ADDRC_DELI_SERV"></core:CustomData>
					<core:CustomData key="inputKey" value="deli_SERV_TYPE"></core:CustomData>
					<core:CustomData key="inputText" value="deli_SERV_DSCR"></core:CustomData>
					<core:CustomData key="deli_SERV_TYPE" value="Delivery Type"></core:CustomData>
					<core:CustomData key="deli_SERV_DSCR" value="Description"></core:CustomData>
				</customData>
			</Input>
			<Label text="Number of Delivery Service"/>
			<Input value="{Customer>/createCRVendorData/formData/parentDTO/customData/gen_adrc/gen_adrc_1/deli_serv_number}"/>
			<Label text="PO Box"/>
			<Input value="{Customer>/createCRVendorData/formData/parentDTO/customData/vnd_lfa1/PFACH}" maxLength="10"/>
			<Label text="Undeliverable"/>
			<Input value="{Customer>/createCRVendorData/formData/parentDTO/customData/gen_adrc/gen_adrc_1/DONT_USE_P}"/>
			<core:Title text="Address"/>
			<Label text="Street / House No."/>
			<Input id="ERPAddrHouseNoId" required="ture" width="100%"
				value="{Customer>/createCRVendorData/formData/parentDTO/customData/gen_adrc/gen_adrc_1/street}"/>
			<Input id="ERPAddrStreetId" required="true" width="100%"
				value="{Customer>/createCRVendorData/formData/parentDTO/customData/gen_adrc/gen_adrc_1/house_num1}">
				<layoutData>
					<l:GridData span="XL2 L3 M3 S4"/>
				</layoutData>
			</Input>
			<Label text="District"/>
			<Input value="{Customer>/createCRVendorData/formData/parentDTO/customData/gen_adrc/gen_adrc_1/city2}"/>
			<Label text="Street 2"/>
			<Input value="{Customer>/createCRVendorData/formData/parentDTO/customData/gen_adrc/gen_adrc_1/str_suppl1}"/>
			<Label text="Street 3"/>
			<Input value="{Customer>/createCRVendorData/formData/parentDTO/customData/gen_adrc/gen_adrc_1/str_suppl2}"/>
			<Label text="Street 4"/>
			<Input value="{Customer>/createCRVendorData/formData/parentDTO/customData/gen_adrc/gen_adrc_1/str_suppl2}"/>
			<Label text="Street 5"/>
			<Input value="{Customer>/createCRVendorData/formData/parentDTO/customData/gen_adrc/gen_adrc_1/location}"/>
			<Label text="Postal Code / City"/>
			<Input id="idERPVendorPostalCode" required="true" value="{Customer>/createCRVendorData/formData/parentDTO/customData/vnd_lfa1/PSTLZ}"
				maxLength="10">
				<layoutData>
					<l:GridData span="XL2 L3 M3 S4"/>
				</layoutData>
			</Input>
			<Input id="idERPVendorCity" required="true" value="{Customer>/createCRVendorData/formData/parentDTO/customData/vnd_lfa1/ORT01}"
				maxLength="35"/>
			<Label text="Country"/>
			<ComboBox required="true" items="{Dropdowns>/T005}"
				selectedKey="{Customer>/createCRVendorData/formData/parentDTO/customData/vnd_lfa1/LAND1}">
				<core:Item key="{Dropdowns>land1}" text="{Dropdowns>landx}"/>
			</ComboBox>
			<core:Title/>
			<!--<Label text="Region"/>-->
			<!--<Input required="true" value="{Customer>/createCRVendorData/formData/parentDTO/customData/vnd_lfa1/REGIO}" maxLength="3"/>-->
			<Label text="Region"/>
			<Input id="idERPVendorRegion" required="true" showValueHelp="true" valueHelpRequest=".onValueHelpRequested" valueHelpOnly="true"
				value="{Customer>/createCRVendorData/formData/parentDTO/customData/vnd_lfa1/REGIO}">
				<customData>
					<core:CustomData key="title" value="Region"></core:CustomData>
					<core:CustomData key="table" value="T005S"></core:CustomData>
					<core:CustomData key="inputKey" value="bland"></core:CustomData>
					<core:CustomData key="inputText" value="bland"></core:CustomData>
					<core:CustomData key="bland" value="Code"></core:CustomData>
					<core:CustomData key="bezei" value="Region"></core:CustomData>
				</customData>
			</Input>
			<Label text="Language"/>
			<ComboBox required="true" selectedKey="{Customer>/createCRVendorData/formData/parentDTO/customData/vnd_lfa1/SPRAS}"
				items="{Dropdowns>/T002}">
				<core:Item key="{Dropdowns>spras}" text="{Dropdowns>sptxt}"/>
			</ComboBox>
			<Label text="Transportation Zone"/>
			<Input value="{Customer>/createCRVendorData/formData/parentDTO/customData/vnd_lfa1/LZONE}" maxLength="10"/>
			<Label text="PO Box Postal Code/ PO Box"/>
			<Input id="idERPPOBOXPOSTALCODE" liveChange="handleERPPOBOXPostalCode"
				value="{Customer>/createCRVendorData/formData/parentDTO/customData/vnd_lfa1/PO_BOX}"/>
			<Label text="PO Box Without No."/>
			<CheckBox select="onSelectCheckBox">
				<customData>
					<core:CustomData key="/gen_adrc/gen_adrc_1/po_box_num" value="X"></core:CustomData>
				</customData>
			</CheckBox>
			<Label text="Company Postal Code"/>
			<Input value="{Customer>/createCRVendorData/formData/parentDTO/customData/gen_adrc/gen_adrc_1/post_code3}"/>
			<Label text="Supplement"/>
			<Input value="{Customer>/createCRVendorData/formData/parentDTO/customData/gen_adrc/gen_adrc_1/house_num2}"/>
		</form:content>
	</form:SimpleForm>
</core:FragmentDefinition>