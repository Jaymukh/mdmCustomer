{
	"_version": "1.12.0",
	"sap.app": {
		"id": "murphy.mdm.customer.murphymdmcustomer",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		},
		"dataSources": {
			"CreateCustDataSource": {
				"uri": "model/localJson.json",
				"type": "JSON"
			},
			"changeRequestMetatDataSource": {
				"uri": "model/ChangeRequestMetaData.json",
				"type": "JSON"
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"fullWidth": true,
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "murphy.mdm.customer.murphymdmcustomer.view.App",
			"type": "XML",
			"async": true,
			"id": "App"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "murphy.mdm.customer.murphymdmcustomer.i18n.i18n"
				}
			},
			"Customer": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "CreateCustDataSource",
				"settings": {
					"defaultBindingMode": "TwoWay"
				}
			},
			
			"SearchCustomerModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {
					"defaultBindingMode": "TwoWay"
				}
			},
			"SearchCustomerPopupModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {
					"defaultBindingMode": "TwoWay"
				}
			},
			"ChangeRequestMetadataModel": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "changeRequestMetatDataSource",
				"settings": {
					"defaultBindingMode": "TwoWay"
				}
			},
			"changeRequestGetAllModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {
					"defaultBindingMode": "TwoWay"
				}
			},
			"changeRequestStatisticsModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {
					"defaultBindingMode": "TwoWay"
				}
			},
			"CreateERPCustomerAddrTableModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {
					"defaultBindingMode": "TwoWay",
					"rows": []
				}
			},
			"userManagementModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {
					"defaultBindingMode": "TwoWay",
					"role": [],
					"accountGroups": [],
					"data": {}
				}
			},
			"Dropdowns": {
				"type": "sap.ui.model.json.JSONModel"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "murphy.mdm.customer.murphymdmcustomer.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [
				{
					"name": "SearchCustomer",
					"pattern": "",
					"target": [
						"SearchCustomer"
					]
				},
				{
					"name": "createCustomer",
					"pattern": "CreateCustomer",
					"target": [
						"createCustomer"
					]
				},
				{
					"name": "erpCustomerPrev",
					"pattern": "CustomerPreview",
					"target": [
						"ERPCustomerPreview"
					]
				}
			],
			"targets": {
				"SearchCustomer": {
					"viewType": "XML",
					"viewName": "SearchCustomer"
				},
				"createVendor": {
					"viewType": "XML",
					"viewName": "CreateCustomer"
				},
				"CreateERPCustomer": {
					"viewType": "XML",
					"viewName": "CreateERPCustomer"
				},
				"ERPCustomerPreview": {
					"viewType": "XML",
					"viewName": "ERPCustomerPreview"
				},
				"ChangeRequest": {
					"viewType": "XML",
					"viewName": "ChangeRequest"
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}